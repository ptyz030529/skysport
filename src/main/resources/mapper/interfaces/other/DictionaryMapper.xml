<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.skysport.core.mapper.DictionaryMapper">
	<sql id="searchDicSql">
			id AS id,
			type AS type,
			key_name  AS keyName,
			value_name   AS valueName
	</sql>
	
	
	<select id="searchDictionary" resultType="Dictionary">
		SELECT 
			<include refid="searchDicSql"/>
		FROM t_sys_dictionary
	</select>
	

</mapper>